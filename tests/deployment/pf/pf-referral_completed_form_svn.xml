<h:html xmlns:h="http://www.w3.org/1999/xhtml"

        xmlns="http://www.w3.org/2002/xforms"

	  xmlns:ev="http://www.w3.org/2001/xml-events"

	  xmlns:xsd="http://www.w3.org/2001/XMLSchema"

	  xmlns:jr="http://openrosa.org/javarosa">

<h:head>

    <h:title>Pathfinder referral</h:title>
    <h:meta jr:name="pathfinder_cc_resolution_0.0.2"/>

    <model>

		<itext>
		
					<translation lang="Swahili">
			
				<text id="referral">

					<value>Je Rufaa imekamilika? </value>

				</text>

				<text id="yes">

					<value>Ndiyo</value>

				</text>

				<text id="no">

					<value>Hapana</value>

				</text>
				
				<text id="why">

					<value>Kwa nini haijakamilika? </value>

				</text>
				
					<text id="no_Time">

					<value>Mgonjwa hakupata muda:</value>

				</text>
				
				<text id="didnt_come_back">

					<value>Mgonjwa hakuleta majibu:</value>

				</text>
				
				<text id="not_Sure">

					<value>Sina uhakika:</value>

				</text>
				
				<text id="when">
					<value>Mgonjwa anategemea kwenda kuhudhuria rufaa lini?</value>
				</text>
				
				<text id="how">
					<value>Umepataje majibu ya rufaa ya mgonjwa?</value>
				</text>
				
				<text id="went_to_client">

					<value>Nimekwenda nyumbani kwa mgonjwa</value>

				</text>
				
				<text id="came_to_me">

					<value>Mgonjwa ameleta majibu kwangu</value>

				</text>
				
								<text id="method">

					<value>Umepata majibu ya rufaa kwa njia gani?</value>

				</text>

				<text id="verbal">

					<value>Kwa njia ya mdomo</value>

				</text>
				
				<text id="doctors_Note">

					<value>Nimepata kipande cha karatasi kutoka kwa Daktari</value>

				</text>
				
				<text id="test_Results">
					<value>Je majibu ya vipimo yakoje?</value>
				</text>
				
				<text id="positive">

					<value>Positive</value>

				</text>
				
				<text id="negative">

					<value>Negative</value>
				</text>
				
				<text id="received_care">

					<value>Amepata Matibabu</value>

				</text>
				
				
				<text id="check_Card">

					<value>Je umehakikisha majibu kwenye cheti cha mgonjwa?</value>

				</text>
				
				<text id="remarks">
					<value>Andika maoni yako</value>
				</text>
				
				
			</translation>
			
			
			<translation lang="English">
			
				<text id="referral">

					<value>Has the referral been completed? </value>

				</text>

				<text id="yes">

					<value>Yes</value>

				</text>

				<text id="no">

					<value>No</value>

				</text>
				
				<text id="why">

					<value>Why not? </value>

				</text>
				
					<text id="no_Time">

					<value>Client didn't have time:</value>

				</text>
				
				<text id="didnt_come_back">

					<value>Client didn't come back:</value>

				</text>
				
				<text id="not_Sure">

					<value>Unsure:</value>

				</text>
				
				<text id="when">
					<value>When is the Client intending to go?</value>
				</text>
				
				<text id="how">
					<value>How did you find out about this referral?</value>
				</text>
				
				<text id="went_to_client">

					<value>I went to Client's house</value>

				</text>
				
				<text id="came_to_me">

					<value>The Client came to me</value>

				</text>
				
								<text id="method">

					<value>Method used to get Referral feedback?</value>

				</text>

				<text id="verbal">

					<value>By Verbal</value>

				</text>
				
				<text id="doctors_Note">

					<value>Seen Doctors Note</value>

				</text>
				
				<text id="test_Results">
					<value>What were the Test results</value>
				</text>
				
				<text id="positive">

					<value>Positive</value>

				</text>
				
				<text id="negative">

					<value>Negative</value>

				</text>
				
				<text id="received_care">

					<value>Received Care</value>

				</text>
				
					
				<text id="check_Card">

					<value>Have you seen the results on the Card?</value>

				</text>
				
				<text id="remarks">
					<value>Remarks</value>
				</text>
				
			</translation>

		</itext>

		<instance>

			<pathfinder_referral xmlns="http://dev.commcarehq.org/Pathfinder/pathfinder_cc_resolution_0.0.2" version="1" uiVersion="1">

				<Meta>

					<formName>PathfinderReferralVisit</formName>

					<formVersion>0.01</formVersion>

					<DeviceID />
					<TimeStart />
					<TimeEnd />
					<username />
					<chw_id />
					<uid />
					<referral_id />
				
					

				</Meta>

				

				<referral />
				<referral_id />
				<why />
				<when />
				<how />
				<method />
				<test_Results />
				<check_Card />
				
				<remarks />
				
			</pathfinder_referral>

		</instance>

		

		<bind id="hidden1" nodeset="/pathfinder_referral/Meta/DeviceID" type="xsd:string" jr:preload="property" jr:preloadParams="DeviceID" />

		<bind id="hidden2" nodeset="/pathfinder_referral/Meta/TimeStart" type="xsd:dateTime" jr:preload="timestamp" jr:preloadParams="start" />

		<bind id="hidden3" nodeset="/pathfinder_referral/Meta/TimeEnd" type="xsd:dateTime" jr:preload="timestamp" jr:preloadParams="end" />

		<bind id="hidden4" nodeset="/pathfinder_referral/Meta/username" type="xsd:string" jr:preload="context" jr:preloadParams="UserName" />
		<bind id="hidden5" nodeset="/pathfinder_referral/Meta/chw_id" type="xsd:string" jr:preload="context" jr:preloadParams="UserID" />
		<bind id="hidden6" nodeset="/pathfinder_referral/Meta/uid" type="xsd:string" jr:preload="uid" jr:preloadParams="general" />
		<bind id="hidden7" nodeset="/pathfinder_referral/referral_id" type="xsd:string" jr:preload="patient_referral" jr:preloadParams="id"/>
		<bind id="referral" nodeset="/pathfinder_referral/referral" type="xsd:string" required="true()" />
		
		
		<bind id="why" nodeset="/pathfinder_referral/why" relevant="/pathfinder_referral/referral = 'no'" />
		
		<bind id="when" nodeset="/pathfinder_referral/when" type="xsd:date" relevant="/pathfinder_referral/referral = 'no'" />
		
		<bind id="how" nodeset="/pathfinder_referral/how" relevant="/pathfinder_referral/referral = 'yes'" />
		
		<bind id="method" nodeset="/pathfinder_referral/method" relevant="/pathfinder_referral/referral = 'yes'" />
		
		<bind id="test_Results" nodeset="/pathfinder_referral/test_Results" relevant="/pathfinder_referral/referral = 'yes'" />
		
		<bind id="seen_Card" nodeset="/pathfinder_referral/check_Card" relevant="/pathfinder_referral/referral = 'yes'" />
		
		<bind nodeset="remarks" relevant="../referral = 'no'" />
		
	</model>

</h:head>			

<h:body>


<select1 ref="/pathfinder_referral/referral">

		<label ref="jr:itext('referral')"></label>

		<item><label ref="jr:itext('yes')"></label><value>yes</value></item>

		<item><label ref="jr:itext('no')"></label><value>no</value></item>

	</select1>
	
		<select1 ref="/pathfinder_referral/why">

		<label ref="jr:itext('why')"></label>

		<item><label ref="jr:itext('no_Time')"></label><value>no_Time</value></item>

		<item><label ref="jr:itext('didnt_come_back')"></label><value>didnt_come_back</value></item>
		<item><label ref="jr:itext('not_Sure')"></label><value>Unsure</value></item>

	</select1>
	
	<input bind="when"><label ref="jr:itext('when')"></label></input>
	<input ref="remarks"><label ref="jr:itext('remarks')"></label></input>	
	
	
		<select1 ref="/pathfinder_referral/how">

		<label ref="jr:itext('how')"></label>

		<item><label ref="jr:itext('went_to_client')"></label><value>went_to_client</value></item>

		<item><label ref="jr:itext('came_to_me')"></label><value>came_to_me</value></item>

	</select1>
	
		<select1 ref="/pathfinder_referral/method">

		<label ref="jr:itext('method')"></label>

		<item><label ref="jr:itext('verbal')"></label><value>Verbal</value></item>

		<item><label ref="jr:itext('doctors_Note')"></label><value>doctors_Note</value></item>

	</select1>
	
	
		<select1 ref="/pathfinder_referral/test_Results">

		<label ref="jr:itext('test_Results')"></label>

		<item><label ref="jr:itext('positive')"></label><value>Positive</value></item>

		<item><label ref="jr:itext('negative')"></label><value>Negative</value></item>
		
		<item><label ref="jr:itext('received_care')"></label><value>received_care</value></item>

	</select1>
	
		<select1 ref="/pathfinder_referral/check_Card">

		<label ref="jr:itext('check_Card')"></label>

		<item><label ref="jr:itext('yes')"></label><value>Yes</value></item>

		<item><label ref="jr:itext('no')"></label><value>No</value></item>

	</select1>

</h:body>

</h:html>

