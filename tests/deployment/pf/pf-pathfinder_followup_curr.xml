		<instance>
			<pathfinder_followup xmlns="http://dev.commcarehq.org/Pathfinder/pathfinder_cc_follow_0.0.2a" version="1" uiVersion="1">
				<Meta>
					<formName>PathfinderFollowUpVisit</formName>
					<formVersion>0.0.1</formVersion>
					<DeviceID />
					<CommCareVersion />
					<TimeStart />
					<TimeEnd />
					<username />
					<chw_id />
					<uid />
					
				</Meta>
				
				<client_name />
				<client_number />
				<type_of_client />
				
				<client_available />
				<why_missing />
				
				<referral />
				
				<new_male_pcg />
				<existing_male_pcg />
				<new_female_pcg />
				<existing_female_pcg />
				
				<type_med />
				
				<mosquito_net/>
				<water_guard />
				
				<service_stopped />
				<date_stopped />
				<reason_stopped />
				
			</pathfinder_followup>
		</instance>
		
		<bind id="hidden1" nodeset="/pathfinder_followup/Meta/DeviceID" type="xsd:string" jr:preload="property" jr:preloadParams="DeviceID" />
		<bind id="hidden7" nodeset="/pathfinder_followup/Meta/CommCareVersion" type="xsd:string" jr:preload="property" jr:preloadParams="cc_version" />
		<bind id="hidden4" nodeset="/pathfinder_followup/Meta/username" type="xsd:string" jr:preload="context" jr:preloadParams="UserName" />
		<bind id="hidden5" nodeset="/pathfinder_followup/Meta/chw_id" type="xsd:string" jr:preload="context" jr:preloadParams="UserID" />
		<bind id="hidden2" nodeset="/pathfinder_followup/Meta/TimeStart" type="xsd:dateTime" jr:preload="timestamp" jr:preloadParams="start" />
		<bind id="hidden3" nodeset="/pathfinder_followup/Meta/TimeEnd" type="xsd:dateTime" jr:preload="timestamp" jr:preloadParams="end" />
		<bind id="hidden6" nodeset="/pathfinder_followup/Meta/uid" type="xsd:string" jr:preload="uid" jr:preloadParams="general" />
		
		<bind id="static1" nodeset="client_name" readonly="true()" jr:preload="patient" jr:preloadParams="fullName" />
		<bind id="static2" nodeset="client_number" readonly="true()" jr:preload="patient" jr:preloadParams="ID" />
		
		<bind id="client_available" nodeset="/pathfinder_followup/client_available" type="xsd:string" required="true()" />
		<bind id="why_missing" nodeset="/pathfinder_followup/why_missing" required="true()" relevant="/pathfinder_followup/client_available='0'" />
		
		<bind id="new_male_pcg" nodeset="/pathfinder_followup/new_male_pcg" type="xsd:int" required="true()" relevant="/pathfinder_followup/client_available='1'" constraint=" .&lt; 10" jr:constraintMsg="Mgonjwa hawezi kuwa na watoa huduma wapya zaidi ya kumi;"/>
		<bind id="existing_male_pcg" nodeset="/pathfinder_followup/existing_male_pcg" type="xsd:int" required="true()" relevant="/pathfinder_followup/client_available='1'" constraint=" .&lt; 10" jr:constraintMsg="Mgonjwa hawezi kuwa na watoa huduma zaidi ya kumi;"/>
		<bind id="new_female_pcg" nodeset="/pathfinder_followup/new_female_pcg" type="xsd:int" required="true()" relevant="/pathfinder_followup/client_available='1'" constraint=" .&lt; 10" jr:constraintMsg="Mgonjwa hawezi kuwa na watoa huduma wapya zaidi ya kumi;"/>
		<bind id="existing_female_pcg" nodeset="/pathfinder_followup/existing_female_pcg" type="xsd:int" required="true()" relevant="/pathfinder_followup/client_available='1'" constraint=" .&lt; 10" jr:constraintMsg="Mgonjwa hawezi kuwa na watoa huduma zaidi ya kumi;"/>
		
		<bind id="type_med" nodeset="/pathfinder_followup/type_med" required="true()" relevant="/pathfinder_followup/client_available='1'" />
		<bind id="referral" nodeset="/pathfinder_followup/referral"  relevant="/pathfinder_followup/service_stopped = '0'"/>
		
		<bind id="mosquito_net" nodeset="/pathfinder_followup/mosquito_net" required="true()" relevant="/pathfinder_followup/client_available='1'" />
		<bind id="water_guard" nodeset="/pathfinder_followup/water_guard" required="true()" relevant="/pathfinder_followup/client_available='1'" />
	
		<bind id="service_stopped" nodeset="/pathfinder_followup/service_stopped" type="xsd:string" required="true()" relevant="/pathfinder_followup/why_missing != 'dead'"/>
		
		<bind id="date_stopped" nodeset="/pathfinder_followup/date_stopped" type="xsd:date" required="true()" relevant="/pathfinder_followup/service_stopped = '1'"/>
		<bind id="reason_stopped" nodeset="/pathfinder_followup/reason_stopped" required="true()" relevant="/pathfinder_followup/service_stopped = '1' and /pathfinder_followup/why_missing != 'dead'" />
	</model>
</h:head>			
<h:body>

	<!-- static info fields -->
	<input ref="client_name"><label ref="jr:itext('client_name')" /></input>
	<input ref="client_number"><label ref="jr:itext('client_number')"></label></input>
	   
	   
	   
	<select1 ref="/pathfinder_followup/type_of_client">
		<label ref="jr:itext('type_of_client')"></label>
		<item><label ref="jr:itext('HIV')"></label><value>HIV</value></item>
		<item><label ref="jr:itext('Chronic Illness')"></label><value>Kusendeka</value></item>
	</select1>
		
	<select1 ref="/pathfinder_followup/client_available">
		<label ref="jr:itext('client_available')"></label>
		<item><label ref="jr:itext('yes')"></label><value>1</value></item>
		<item><label ref="jr:itext('no')"></label><value>0</value></item>
	</select1>
	
	<select1 bind="why_missing">
		<label ref="jr:itext('why_missing')"></label>
		<item><label ref="jr:itext('dead')"></label><value>dead</value></item>
		<item><label ref="jr:itext('moved')"></label><value>moved</value></item>
		<item><label ref="jr:itext('not_home')"></label><value>not_home</value></item>
	</select1>
				
	<input bind="new_male_pcg"><label ref="jr:itext('new_male_pcg')"></label></input>
	<input bind="existing_male_pcg"><label ref="jr:itext('existing_male_pcg')"></label></input>
	<input bind="new_female_pcg"><label ref="jr:itext('new_female_pcg')"></label></input>
	<input bind="existing_female_pcg"><label ref="jr:itext('existing_female_pcg')"></label></input>
	
	<select bind="type_med">
		<label ref="jr:itext('type_med')"></label>
		<item><label ref="jr:itext('sep')"></label><value>Septrini</value></item>
		<item><label ref="jr:itext('arv')"></label><value>ARV</value></item>
		<item><label ref="jr:itext('anti_tb')"></label><value>Anti_TB</value></item>
		<item><label ref="jr:itext('kinga_tb')"></label><value>Kinga_ya_TB</value></item>
		<item><label ref="jr:itext('local_med')"></label><value>Local_medicine</value></item>
		<item><label ref="jr:itext('other')"></label><value>other</value></item>
	</select>
	
	<select1 bind="mosquito_net">
		<label ref="jr:itext('mosquito_net')"></label>
		<item><label ref="jr:itext('no')"></label><value>0</value></item>
		<item><label ref="jr:itext('yes')"></label><value>1</value></item>
	</select1>
	
	<select1 bind="water_guard">
		<label ref="jr:itext('water_guard')"></label>
		<item><label ref="jr:itext('no')"></label><value>0</value></item>
		<item><label ref="jr:itext('yes')"></label><value>1</value></item>
	</select1>
	
	<select1 bind="service_stopped">
		<label ref="jr:itext('service_stopped')"></label>
		<item><label ref="jr:itext('no')"></label><value>0</value></item>
		<item><label ref="jr:itext('yes')"></label><value>1</value></item>
	</select1>
	
	<input bind="date_stopped"><label ref="jr:itext('date_stopped')"></label></input>

	<select1 bind="reason_stopped">
		<label ref="jr:itext('reason_stopped')"></label>
		<item><label ref="jr:itext('moved')"></label><value>moved</value></item>
		<item><label ref="jr:itext('refused')"></label><value>refused</value></item>
		<item><label ref="jr:itext('cured')"></label><value>cured</value></item>
	</select1>
	<select bind="referral">
		<label ref="jr:itext('referral')"></label>
		<item><label ref="jr:itext('ref1')"></label><value>hiv_test</value></item>
		<item><label ref="jr:itext('ref2')"></label><value>HIV_other_illness</value></item>
		<item><label ref="jr:itext('ref3')"></label><value>Health_facility</value></item>
		<item><label ref="jr:itext('ref4')"></label><value>Prevention_from_mother_to_child</value></item>
		<item><label ref="jr:itext('ref5')"></label><value>Aid_from_other_groups</value></item>
		<item><label ref="jr:itext('ref6')"></label><value>Family_planning</value></item>
		<item><label ref="jr:itext('ref7')"></label><value>TB</value></item>
		<item><label ref="jr:itext('ref8')"></label><value>Ophans</value></item>
	</select>
	
</h:body>
</h:html>
